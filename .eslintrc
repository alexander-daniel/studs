{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parser-options": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "forOf": true,
    "generators": true,
    "modules": true,
    "templateStrings": true
  },
  "rules": {
    "accessor-pairs": [1],
    "arrow-spacing": [2],
    "block-scoped-var": [2],
    "consistent-return": [0],
    "curly": [0],
    "dot-location": [0],
    "dot-notation": [1],
    "eqeqeq": [2],
    "default-case": [2],
    "guard-for-in": [2],
    "no-alert": [2],
    "no-caller": [2],
    "no-console": [0],
    "no-case-declarations": [2],
    "no-div-regex": [2],
    "no-else-return": [1],
    "no-empty-pattern": [2],
    "no-eq-null": [2],
    "no-eval": [2],
    "no-extend-native": [2],
    "no-extra-bind": [2],
    "no-floating-decimal": [1],
    "no-implicit-coercion": [1],
    "no-implied-eval": [2],
    "no-invalid-this": [0],
    "no-iterator": [2],
    "no-labels": [2],
    "no-lone-blocks": [2],
    "no-loop-func": [2],
    "no-magic-numbers": [0],
    "no-multi-spaces": [1],
    "no-multi-str": [2],
    "no-native-reassign": [2],
    "no-new-func": [2],
    "no-new-wrappers": [2],
    "no-new": [2],
    "no-param-reassign": [0],
    "no-process-env": [0],
    "no-proto": [2],
    "no-return-assign": [2],
    "no-script-url": [2],
    "no-self-compare": [2],
    "no-sequences": [2],
    "no-throw-literal": [2],
    "no-unused-expressions": [2],
    "no-useless-call": [2],
    "no-useless-concat": [2],
    "no-warning-comments": [0],
    "no-with": [2],
    "radix": [2],
    "vars-on-top": [0],
    "wrap-iife": [2],
    "yoda": [2],
    "key-spacing": [0],
    "new-cap": [0],
    "no-shadow": [0],
    "no-undefined": [0],
    "no-underscore-dangle": [0],
    "no-unused-vars": [1],
    "no-use-before-define": [2, "nofunc"],
    "semi": [2, "always"],
    "quotes": [1, "single", "avoid-escape"],
    "valid-jsdoc": [1],
    "react/jsx-closing-bracket-location": [1, "line-aligned"],
    "react/jsx-uses-vars": [1],
    "react/jsx-quotes": 0,
    "require-yield": 0,
    "jsx-quotes": [2, "prefer-double"],
    "keyword-spacing": 2,
    "space-infix-ops": 2
  },
  "globals": {
    "jest": true,
    "it": true,
    "expect": true,
    "beforeEach": true,
    "describe": true,
    "React": true,
    "test": true,
    "jasmine": true
  },
  "plugins": [
    "react"
  ]
}
